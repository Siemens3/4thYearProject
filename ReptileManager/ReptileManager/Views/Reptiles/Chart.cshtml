@model Charts

 
<div class="well">
    <p style="font-size:18px">Statistics</p>
    <div class="row">
        <div class="well" style="background-color: #fff">
            <p style="font-size:18px;">Weight (@DateTime.Now.Year)</p>
            @ViewBag.Test
            
            @(Model.Chart1)
        </div>
    </div>
    <br />

    <div class="row">
        <div class="well" style="background-color: #fff">
            <p style="font-size:18px;">Length (@DateTime.Now.Year)</p>
            @(Model.Chart3)
        </div>
    </div>
    <br />

    <div class="row">
        <div class="well" style="background-color: #fff">
            <p style="font-size:18px;">Feedings (@DateTime.Now.Year)</p>
            @(Model.Chart2)
        </div>
    </div>
</div>